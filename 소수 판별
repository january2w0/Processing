import java.util.Arrays;

int n = 12397;  // 판별할 수 입력
int i = 0;
int x = 0;
ArrayList prime = new ArrayList();

void setup() {
  println("Number:",n);
  for (i=1; i<n; i++) {
    if (n%i == 0) {
      x++;
      prime.add(x-1, i);
    }
  }
  print("Number of factors:", x, " ");
  System.out.println(prime.toString());
  if (x == 1) {
    print("This is prime number!");
  } else {
    print("This is composition number!");
  }
}

void draw() {

}
